! Xft
! Xft.dpi: 96
! Xft.antialias: true
! Xft.hinting: true
! Xft.autohint = false
! Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

! UXTerm
! UXTerm.vt100.geometry: 125x30
*XTerm.vt100.locale: true
*XTerm.vt100.metaSendsEscape: true
*XTerm.vt100.saveLines: 4096
*XTerm.vt100.scrollBar: true
*XTerm.vt100.rightScrollBar: true
*XTerm.vt100.scrollKey: true
*XTerm.vt100.scrollTtyOutput: false
*XTerm.vt100.jumpScroll: true
*XTerm.vt100.multiScroll: true
*XTerm.vt100.toolBar: false
*XTerm*faceName: DejaVu Sans Mono
*XTerm*faceNameDoublesize: Source Han Sans CN
*XTerm*faceSize: 10
*XTerm.vt100.cursorBlink: true
*XTerm.vt100.bellIsUrgent: true
*XTerm.vt100.eightBitInput: true

!TomorrowNightEighties
*.foreground:   #cccccc
*.background:   #2d2d2d
*.cursorColor:  #aeafad
! Black + Grey
*.color0:       #000000
*.color8:       #666666
! Red + Bright Red
*.color1:       #f2777a
*.color9:       #FF3334
! Green + Bright Green
*.color2:       #99cc99
*.color10:      #9ec400
! Yellow (Orange) + Bright Yellow (Yellow)
*.color3:       #f99157
*.color11:      #ffcc66
! Blue + Bright Blue
*.color4:       #6699cc
*.color12:      #6699cc
! Magenta (Purple) + Bright Magenta
*.color5:       #cc99cc
*.color13:      #b777e0
! Cyan (Aqua) + Bright Cyan
*.color6:       #66cccc
*.color14:      #54ced6
! Light Grey (Selection) + White (Current Line)
*.color7:       #515151
*.color15:      #393939
